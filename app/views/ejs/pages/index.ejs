<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('../partials/head') %>
</head>
<body>
  <header>
    <%- include('../partials/header') %>
  </header>
  <main>

    <!-- Inserto el contenido dinamico-->

    <table class="table">
      <thead>
        <tr>
          <th scope="col">ID</th>
          <th scope="col">Title</th>
          <th scope="col">Price</th>
          <th scope="col">URL de Foto</th>
        </tr>
      </thead>
    
      <tbody>
        <% productos.forEach((prod) => {  %>
        <tr> 
          <th scope="row"> <%= prod.id %> </th>
          <td> <%= prod.title %> </td>
          <td> <%= prod.price %> </td>
          <td>
          <img src= <%= prod.thumbnail %> alt=<%= prod.title %> style="width: 100px; height:100px"></td> 
        </tr>
            <% }) %>
      </tbody>
    </table>

  </main>
  <footer>
    <%- include('../partials/footer') %>
  </footer>
</body>
</html>